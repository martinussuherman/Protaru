<div class="col-sm-6 col-md-1 form-group p-2px">
    <select class="form-control" name="AtrDokumenList[@Index].Status">
        <option value="0" selected="@(!StatusAda)">Tidak</option>
        <option value="1" selected="@StatusAda">Ada</option>
    </select>
</div>

@code {
    [Parameter]
    public int Index { get; set; }

    [Parameter]
    public bool StatusAda { get; set; }
}
