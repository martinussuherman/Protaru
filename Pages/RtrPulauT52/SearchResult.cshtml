@page "{handler?}"
@model SearchResultModel

@{
    Layout = "_RtrNasionalLayout";
    ViewData["PageTitle"] = "RTR Pulau/Kepulauan";
    ViewData["Title"] = "Monitoring & Evaluasi RTR Pulau/Kep";
    var pagingHtml = await Html.PartialAsync("_PagingPartial", Model.Hasil);
}

<component type="typeof(SearchResultPage)"
    render-mode="Static"
    param-PageTitle="@ViewData["PageTitle"]"
    param-Title="@ViewData["Title"]"
    param-Model="@Model"
    param-DisplayedLocation="@RtrLocationEnum.Pulau"
    param-PagingHtml="@pagingHtml" />
