@page
@model MonevAtr.Pages.Rdtr.ImportModel

@{
    ViewData["Title"] = "Impor Data RDTR";
}

<div class="page-header">
    <h1 class="page-title"></h1>
</div>
<div class="row row-cards text col-12 card">
    <div class="card-header">
        <h3 class="card-title"> Impor Data </h3>
    </div>
    <form method="post" enctype="multipart/form-data">
        <div class="card-body row">
                <div class="col-md-12 form-group">
                    <div class="custom-file">
                        <input asp-for="ImportFile" class="custom-file-input" />
                        <label class="custom-file-label">Choose file</label>
                    </div>
                </div>
        </div>
        <div class="card-footer text-center">
            <button type="submit" class="btn btn-primary"><i class="fe fe-search"></i> Impor</button>
        </div>
    </form>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

