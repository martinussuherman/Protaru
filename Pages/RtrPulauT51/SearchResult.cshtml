@page "{handler?}"
@model SearchResultModel

@{
    Layout = "_RtrNasionalSearchResultLayout";
    ViewData["PageTitle"] = "RTR Pulau/Kepulauan";
    ViewData["Title"] = Model.IsCanEdit ? 
        "Monitoring & Evaluasi RTR Pulau/Kep T5-1" : 
        "Monitoring & Evaluasi RTR Pulau/Kep";
    string pageName = Model.IsCanEdit ? "./Edit" : "./View";
}

<div class="card-body table-responsive">
    <component type="typeof(SearchResultProvKabKota)" 
        render-mode="Static" 
        param-PageName="@pageName" 
        param-RegulationName="@("Perpres")"
        param-DisplayedLocation="@RtrTableLocationEnum.Pulau"
        param-IsCanEdit="@Model.IsCanEdit" 
        param-IsDisplayRegulation="true" 
        param-Url="@Model.Url" 
        param-Items="@Model.Hasil" />
    <partial name="_PagingPartial" model="Model.Hasil" />
</div>
