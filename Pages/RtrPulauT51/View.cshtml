@page "{id:int?}"
@model ViewModel

@{
    Layout = "_RtrViewLayout";
    ViewData["Title"] = "Detail RTR Pulau/Kep T5-1";
    ViewData["PageTitle"] = "RTR Pulau/Kepulauan";
}

@await Html.PartialAsync("_RtrViewTitlePartial", Model.RtrDetail)

<div class="justify-content-md-center px-5">
    <div class="card border-orange mb-3 card-body">
        @await Html.PartialAsync("_RtrViewCommonHeaderPartial", Model.RtrDetail)
    </div>
</div>

@await Html.PartialAsync("_RtrViewDokumenPartial", Model.RtrDetail)
