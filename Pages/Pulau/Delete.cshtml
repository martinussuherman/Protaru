@page "{id:int?}"
@model DeleteModel

@{
    Layout = "_CommonLayout";
    ViewData["Title"] = "Hapus Pulau";
    ViewData["PageTitle"] = "Pulau";
}

<div class="card-body">
    <div class="form-group">
        <label class="control-label">Kode</label>
        <input asp-for="Pulau.Kode" class="form-control" disabled/>
    </div>

    <div class="form-group">
        <label class="control-label">Nama</label>
        <input asp-for="Pulau.Nama" class="form-control" disabled/>
    </div>

    <form method="post">
        <input type="hidden" asp-for="Pulau.Kode" />
        <input type="submit" value="Hapus" class="btn btn-danger" />
        <a asp-page="./Index" class="btn btn-warning">Kembali</a>
    </form>
</div>
