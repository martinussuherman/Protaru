@page "{handler?}"
@model SearchResultModel

@{
    Layout = "_RtrNasionalSearchResultLayout";
    ViewData["Title"] = "Monitoring & Evaluasi RTR KSN";
    ViewData["PageTitle"] = "RTR KSN";
}

<div class="card-body table-responsive">
    <component type="typeof(SearchResultTable)" 
        render-mode="Static" 
        param-RegulationName="@("Perpres")"
        param-DisplayedLocation="@RtrLocationEnum.Kawasan"
        param-IsCanEdit="false" 
        param-IsDisplayRegulation="@Model.IsPerdaPerpres" 
        param-Url="@Model.Url" 
        param-Items="@Model.Hasil" />
    <partial name="_PagingPartial" model="Model.Hasil" />
</div>
